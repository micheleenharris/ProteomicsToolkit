<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <display-name>Proteomics Toolkit</display-name>
  <description>
     A set of proteomics tools to fiddle with peptide properties.
  </description>

  <servlet>
    <servlet-name>FragIonServlet</servlet-name>
    <servlet-class>FragIonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FragIonServlet</servlet-name>
    <url-pattern>/FragIonServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ProteinDigest</servlet-name>
    <servlet-class>ProteinDigest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProteinDigest</servlet-name>
    <url-pattern>/ProteinDigest</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DNAServlet</servlet-name>
    <servlet-class>DNAServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DNAServlet</servlet-name>
    <url-pattern>/DNAServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>IsotopeServlet</servlet-name>
    <servlet-class>IsotopeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IsotopeServlet</servlet-name>
    <url-pattern>/IsotopeServlet</url-pattern>
  </servlet-mapping>

</web-app>
